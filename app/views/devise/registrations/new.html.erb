<%= image_tag("user.png", :class => "style_image") %>

<div class="row">
  <div class="col s4">  </div>
  <div class="col s4">
      
      
      <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
        <%= devise_error_messages! %>
      
        <div class="input-field">
          <%= f.email_field :email, autofocus: true, id:"email_" %>
          <label for="email_" class="active">Email</label>
        </div>
      
        <div class="input-field">
          
          <%= f.password_field :password, autocomplete: "off",id: "pass_" %>
          <label for="pass_" class="active">Password</label>
        </div>
      
        <div class="input-field">
          <%= f.password_field :password_confirmation, autocomplete: "off",id:"pass_conf" %>
          <label for="pass_conf" class="active">Password confirmation</label>
        </div>
      
        <div class="actions">
          <%= f.submit "Sign up", class:"waves-effect waves-light btn" %>

        </div>
      <% end %>   

  </div>
</div>

