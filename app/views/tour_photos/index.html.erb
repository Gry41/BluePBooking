<p id="notice"><%= notice %></p>

<h1>Tour Photos</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tour_photos.each do |tour_photo| %>
      <tr>
        <td><%= link_to 'Show', tour_photo %></td>
        <td><%= link_to 'Edit', edit_tour_photo_path(tour_photo) %></td>
        <td><%= link_to 'Destroy', tour_photo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tour Photo', new_tour_photo_path %>
