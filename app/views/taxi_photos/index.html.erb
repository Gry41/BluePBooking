<p id="notice"><%= notice %></p>

<h1>Taxi Photos</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @taxi_photos.each do |taxi_photo| %>
      <tr>
        <td><%= link_to 'Show', taxi_photo %></td>
        <td><%= link_to 'Edit', edit_taxi_photo_path(taxi_photo) %></td>
        <td><%= link_to 'Destroy', taxi_photo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Taxi Photo', new_taxi_photo_path %>
