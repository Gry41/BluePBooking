
<div class="container">
<h3>Car types</h3>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cartypes.each do |cartype| %>
      <tr>
        <td><%= cartype.name %></td>
        <td><%= link_to 'Edit', edit_cartype_path(cartype), class:"waves-effect waves-light btn", id:"btn-small" %>|<%= link_to 'Destroy', cartype, method: :delete, data: { confirm: 'Are you sure?' },class:"waves-effect waves-light btn", id:"btn-small" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cartype', new_cartype_path,class:"waves-effect waves-light btn" %>
</div>