<p id="notice"><%= notice %></p>

<h1>Rent Images</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rent_images.each do |rent_image| %>
      <tr>
        <td><%= link_to 'Show', rent_image %></td>
        <td><%= link_to 'Edit', edit_rent_image_path(rent_image) %></td>
        <td><%= link_to 'Destroy', rent_image, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Rent Image', new_rent_image_path %>
