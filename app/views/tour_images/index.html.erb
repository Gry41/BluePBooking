<p id="notice"><%= notice %></p>

<h1>Tour Images</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tour_images.each do |tour_image| %>
      <tr>
        <td><%= link_to 'Show', tour_image %></td>
        <td><%= link_to 'Edit', edit_tour_image_path(tour_image) %></td>
        <td><%= link_to 'Destroy', tour_image, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tour Image', new_tour_image_path %>
